{
    "require-dev": {
        "caseyamcl/phpoaipmh": "^3.0",
        "collectiveaccess/service-wrapper": "v1.1.3",
        "dompdf/dompdf": "~0.8.5",
        "easyrdf/easyrdf": "0.9.*",
        "elasticsearch/elasticsearch": "~5.0",
        "ezyang/htmlpurifier": "~4.9",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.4",
        "hoa/compiler": "~3.17",
        "hoa/visitor": "~2.17",
        "james-heinrich/getid3": "^1.9.20",
        "joshralph/password-policy": "~0.2",
        "knplabs/github-api": "~2.7",
        "laminas/laminas-escaper": "*",
        "laminas/laminas-stdlib": "*",
        "league/climate": ">=1.0.0",
        "league/flysystem": "v1.0.45",
        "league/flysystem-aws-s3-v3": ">=1.0.0",
        "league/flysystem-cached-adapter": ">=1.0.0",
        "league/flysystem-webdav": ">=1.0.0",
        "league/flysystem-ziparchive": ">=1.0.0",
        "magento/zendframework1": "=1.13.1",
        "maximebf/debugbar": ">=1.0.0",
        "monolog/monolog": "~1.0",
        "onelogin/php-saml": ">=2.0",
        "opis/json-schema": "^1.0",
        "pear/file_marc": "~1.2",
        "php-http/guzzle6-adapter": "~1.1.1",
        "phpoffice/phppresentation": "dev-master",
        "phpoffice/phpspreadsheet": "v1.10",
        "phpoffice/phpword": "v0.15.*",
        "scholarslab/bagit": "*",
        "srmklive/flysystem-dropbox-v2": ">=1.0.0",
        "symfony/browser-kit": "5.*",
        "symfony/css-selector": "5.*",
        "symfony/debug-pack": "*",
        "symfony/dom-crawler": "^5.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "tedivm/stash": "0.14.*"
    },
    "require": {
        "symfony/asset": "5.*",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/google-mailer": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/intl": "^5.0",
        "symfony/mailer": "^5.0",
        "symfony/messenger": "5.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/notifier": "^5.0",
        "symfony/orm-pack": "*",
        "symfony/process": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/sendgrid-mailer": "5.*",
        "symfony/serializer-pack": "*",
        "symfony/string": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-pack": "*",
        "symfony/validator": "^5.0",
        "symfony/web-link": "^5.0",
        "symfony/yaml": "^5.0",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.0"
    },
    "config": {
        "discard-changes": true,
        "league/climate": ">=1.0.0",
        "james-heinrich/getid3": "^1.9.20",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.*"
        }
    },
    "repositories": {
        "git-custom-index": {
            "type": "vcs",
            "url": "https://github.com/survos/custom-index-bundle.git"
        },
        "adminlte": {
            "type": "vcs",
            "url": "git@github.com:tacman/AdminLTEBundle.git"
        },
        "survos_base_bundle": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        }
    }
}
